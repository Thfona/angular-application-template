{
  "name": "your-project-name",
  "version": "1.0.0",
  "description": "Front-End for the YOUR-PROJECT-NAME website.",
  "author": "Thfona",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "ng serve --port 4000",
    "dev:ssr": "ng run your-project-name:serve-ssr",
    "dev:docker": "docker-compose --env-file ./docker.dev.env up -d --build",
    "start": "node dist/server/main.js",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:ssr": "ng build --prod && ng run your-project-name:server:production",
    "prerender": "ng run your-project-name:prerender",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint && npm run prettier",
    "lint:fix": "ng lint --fix && npm run prettier:fix",
    "prettier": "prettier --config ./.prettierrc --check \"./**/*{.ts,.js,.json,.css,.scss}\"",
    "prettier:fix": "prettier --config ./.prettierrc --check \"./**/*{.ts,.js,.json,.css,.scss}\" --write"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@angular/animations": "10.0.11",
    "@angular/common": "10.0.11",
    "@angular/compiler": "10.0.11",
    "@angular/core": "10.0.11",
    "@angular/forms": "10.0.11",
    "@angular/platform-browser": "10.0.11",
    "@angular/platform-browser-dynamic": "10.0.11",
    "@angular/platform-server": "10.0.11",
    "@angular/router": "10.0.11",
    "@auth0/angular-jwt": "5.0.1",
    "@ngneat/transloco": "2.19.1",
    "@nguniversal/express-engine": "10.0.2",
    "bootstrap": "4.5.2",
    "express": "4.17.1",
    "font-awesome": "4.7.0",
    "ngx-progressbar": "6.0.3",
    "rxjs": "6.5.5",
    "tslib": "2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.2",
    "@angular/cli": "10.0.7",
    "@angular/compiler-cli": "10.0.11",
    "@angular/language-service": "10.0.14",
    "@nguniversal/builders": "10.0.2",
    "@types/express": "4.17.0",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "codelyzer": "6.0.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "5.0.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.2",
    "karma-jasmine": "3.3.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "pre-commit": "1.2.2",
    "prettier": "2.1.0",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "typescript": "3.9.5"
  }
}
